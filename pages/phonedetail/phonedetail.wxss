.warp {
  margin: 20rpx;
  height: auto;
  padding: 20rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 4rpx 16rpx 0px rgba(140, 160, 181, 0.2);
  border-radius: 12rpx;
  box-sizing: border-box;
  overflow: hidden;

}

.list {
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-direction: row;
  line-height: 3; 
}

.title {
  color: black;
  font-size: 30rpx;
  flex-wrap: wrap;
}

.number {
  display: flex;
  flex-direction: row;
  justify-content: center;
  flex-wrap: wrap;
  margin-right: 20rpx;
  font-size: 30rpx;
  color: #007aff;
  text-decoration: underline;
}

.borderline{
  width: 100%;
  position: relative;

  background-color: #e5e5e5;
  height: 1rpx;
}
