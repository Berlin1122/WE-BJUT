<view class="nav">
  <block wx:for="{{student}}">
    <view class="item">
      <image src="{{item.icon}}" id="{{index}}" catchtap='touchIcon'></image>
      <view>\n</view>
      <text>{{item.title}}</text>
    </view>
  </block>
</view>


