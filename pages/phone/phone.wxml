<view class="warp">

<!--循环输出列表 begin-->
<view wx:for="{{phoneList}}" class="list" wx:for-index='id'>
  <view class="infos">
      <view class="title">{{item.name}}</view>
      <view catchtap="callmeTap" class="date"  data-phone="{{item.phonenumber}}">{{item.phonenumber}}</view>
    </view>
    <view class='borderline'></view>
</view>

<loading hidden="{{loadHidden}}" bindchange="loadChange">
    数据加载中...
</loading>

<view bindtap="loadMore" data-lastid="{{lastid}}" data-isfrist="{{isfrist}}" class="loadMore" style="display:{{moreHidden}}">加载更多</view>
<toast hidden="{{toastHidden}}" bindchange="toastChange" duration="3000">{{msg}}</toast>
<modal title="温馨提示" no-cancel hidden="{{confirmHidden}}" confirm-text="明确" bindconfirm="modalChange">你当前不在在WIFI网格下下，会产生流量费用</modal>
</view>

  <button class='btn1'  
session-from="bot | {{nickName}} | {{avatarUrl}}"  open-type='contact'>
    <image src='../../images/jiqiren.png' mode="scaleToFill"></image>
    <text>向我吐槽</text>
 </button>
